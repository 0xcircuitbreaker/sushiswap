{
  "name": "@sushiswap/extractor",
  "version": "0.0.0",
  "private": true,
  "description": "Sushi Extractor",
  "keywords": [
    "sushi",
    "extractor"
  ],
  "homepage": "https://www.sushi.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/sushiswap/sushiswap.git",
    "directory": "packages/extractor"
  },
  "license": "UNLICENSED",
  "author": "Ilya Lyalin",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
